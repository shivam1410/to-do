 node {
     stage('check SCM'){
         git branch: 'master', url: 'git@github.com:shivam1410/to-do.git'
     }

     stage('install node module'){
         sh "npm install"
     }

     stage('build'){
         sh "ng build --prod"
     }

     stage('Deploy'){
	 sh "cp -fr dist /home/shivam/Desktop/tomcat/webapps/to-do/"
     }
 }
